{
  "name": "@kyuzan/mint-sdk-js",
  "description": "MINT NFT Platform Client SDK",
  "version": "8.0.0",
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "agentkeepalive": "^4.1.4",
    "axios": "^0.24.0",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.0.31",
    "fortmatic": "^2.2.1",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "16.11.20",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "esbuild": "0.14.11",
    "eslint": "8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "27.4.7",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "27.1.3",
    "typedoc": "0.22.10",
    "typedoc-plugin-markdown": "3.11.11",
    "typescript": "4.5.4"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "run-p watch:*",
    "doc:deploy": "cd ./docs && npm run deploy",
    "doc:serve": "cd ./docs && npm run start",
    "build:develop": "npm run clean && tsc",
    "build:production": "npm run clean && npm run type:check && tsc",
    "clean": "rimraf lib",
    "watch:typeCheck": "tsc --watch -noEmit --project tsconfig.json",
    "watch:build": "tsc -w",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint:check": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "publish:main": "npm run build:production && npm version major && npm publish",
    "publish:minor": "npm run build:production && npm version minor && npm publish",
    "publish:patch": "npm run build:production && npm version patch && npm publish",
    "publish:alpha": "npm run build:production && npm version premajor --preid alpha && npm publish",
    "publish:alpha:next": "npm run build:production && npm version prerelease && npm publish"
  },
  "repository": "https://github.com/KyuzanInc/annapurna-sdk-js",
  "homepage": "https://github.com/KyuzanInc/annapurna-sdk-js",
  "keywords": [
    "NFT"
  ]
}
