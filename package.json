{
  "name": "@kyuzan/mint-sdk-js",
  "description": "Mint NFT Platform Client SDK",
  "version": "8.9.0-alpha.4",
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@stripe/stripe-js": "^1.22.0",
    "@toruslabs/torus-embed": "^1.20.2",
    "@walletconnect/web3-provider": "^1.7.1",
    "agentkeepalive": "^4.1.4",
    "axios": "^0.27.0",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.6.2",
    "qs": "^6.10.1",
    "web3modal": "1.9.8"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/jest": "28.1.4",
    "@types/node": "16.11.43",
    "@types/qs": "6.9.7",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "auto-changelog": "2.4.0",
    "esbuild": "0.14.48",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.5",
    "typechain": "^8.1.0",
    "typedoc": "0.23.5",
    "typedoc-plugin-markdown": "3.13.3",
    "typescript": "4.7.4"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "run-p watch:*",
    "build:develop": "npm run clean && tsc",
    "build:production": "npm run clean && npm run type:check && tsc",
    "clean": "rimraf lib",
    "watch:typeCheck": "tsc --watch -noEmit --project tsconfig.json",
    "watch:build": "tsc -w",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint:check": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "publish:alpha:major": "npm run build:production && npm version premajor --preid alpha && npm publish",
    "publish:alpha:minor": "npm run build:production && npm version preminor --preid alpha && npm publish",
    "publish:alpha:next": "npm run build:production && npm version prerelease && npm publish",
    "prepublishOnly": "git push --follow-tags",
    "version": "auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:' && git add CHANGELOG.md",
    "typechain": "typechain --out-dir=src/contracts/typechain --target=ethers-v5 './src/contracts/abi/**/*.json'"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/KyuzanInc/mint-sdk-js.git"
  },
  "homepage": "https://github.com/KyuzanInc/mint-sdk-js",
  "keywords": [
    "NFT"
  ]
}
