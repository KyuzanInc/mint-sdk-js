{
  "name": "@kyuzan/mint-sdk-js",
  "description": "MINT NFT Platform Client SDK",
  "version": "3.2.2",
  "dependencies": {
    "agentkeepalive": "^4.1.4",
    "axios": "^0.21.1",
    "ethers": "^5.0.31",
    "fortmatic": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.45",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "esbuild": "^0.8.57",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.8.1",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist"
  ],
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "run-p watch:*",
    "doc:deploy": "cd ./docs && npm run deploy",
    "doc:serve": "cd ./docs && npm run start",
    "build:develop": "tsc --project tsconfig.json",
    "build:production": "tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "watch:build": "tsc --watch --project tsconfig.json",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint:check": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "publish:main": "npm run build:production && npm version major && npm publish",
    "publish:minor": "npm run build:production && npm version minor && npm publish",
    "publish:patch": "npm run build:production && npm version patch && npm publish"
  },
  "repository": "https://github.com/KyuzanInc/annapurna-sdk-js",
  "homepage": "https://github.com/KyuzanInc/annapurna-sdk-js",
  "keywords": [
    "NFT"
  ]
}
