{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "deploy": "now --prod",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "lint:check": "eslint src --ext ts,tsx --resolve-plugins-relative-to ./example",
    "lint:fix": "eslint src --ext ts,tsx --fix --resolve-plugins-relative-to ./example"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@kyuzan/annapurna-sdk-js": "^0.33.0",
    "date-fns": "^2.19.0",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "typescript": "4.0"
  },
  "license": "MIT"
}
